{
  "name": "dry-replacer-js",
  "version": "1.0.11",
  "description": "A simple and lightweight library to replace variables in the JSON template",
  "author": "Thadeu Esteves<tadeuu@gmail.com>",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.umd.js",
  "repository": {
    "url": "https://github.com/thadeu/dry-replacer-js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npx jest --runInBand --detectOpenHandles --silent=false --verbose false",
    "test:build": "npm run build && npx jest --runInBand --detectOpenHandles --silent=false --verbose false",
    "build": "rm -rf dist && microbundle src/index.ts"
  },
  "keywords": [
    "JSON template",
    "JSON replacer",
    "replace variable JSON",
    "regex json template",
    "json",
    "template"
  ],
  "devDependencies": {
    "@sucrase/jest-plugin": "^3.0.0",
    "@types/jest": "^29.5.1",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isarray": "^4.0.7",
    "@types/lodash.isplainobject": "^4.0.7",
    "@types/lodash.set": "^4.3.7",
    "@types/node": "^18.11.18",
    "jest": "^29.6.2",
    "microbundle": "^0.15.1",
    "rimraf": "^4.1.2",
    "sucrase": "^3.29.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.isarray": "^4.0.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.set": "^4.3.2"
  }
}
